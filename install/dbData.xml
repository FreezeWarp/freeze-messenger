<dbData version="3.0">
  <database>

    <!-- Table bbcode -->
    <table name="bbcode">
      <column name="bbcodeId">1</column>
      <column name="bbcodeName">Bold</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(b|strong)](.+?)[/(b|strong)]/is</column>
      <column name="replacement">&lt;span style="font-weight: bold;"&gt;$2&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">2</column>
      <column name="bbcodeName">Strikethrough</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(s|strike)](.+?)[/(s|strike)]/is</column>
      <column name="replacement">&lt;span style="text-decoration: line-through;"&gt;$2&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">3</column>
      <column name="bbcodeName">Italics</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(i|em)](.+?)[/(i|em)]/is</column>
      <column name="replacement">&lt;span style="font-style: oblique;"&gt;$2&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">4</column>
      <column name="bbcodeName">Underline</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(u)](.+?)[/(u)]/is</column>
      <column name="replacement">&lt;span style="text-decoration: underline;"&gt;$2&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">5</column>
      <column name="bbcodeName">Link</column>
      <column name="options">1</column>
      <column name="searchRegex">/[url]([^"&lt;&gt;]*?)[/url]/is</column>
      <column name="replacement">&lt;a href="$1" target="_BLANK"&gt;$1&lt;/a&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">6</column>
      <column name="bbcodeName">Email</column>
      <column name="options">1</column>
      <column name="searchRegex">/[email](.*?)[/email]/is</column>
      <column name="replacement">&lt;a href="mailto:$1"&gt;$1&lt;/a&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">7</column>
      <column name="bbcodeName">Colour</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(color|colour)=("|)(.*?)("|)](.*?)[/(color|colour)]/is</column>
      <column name="replacement">&lt;span style="color: $3;"&gt;$5&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">8</column>
      <column name="bbcodeName">Highlight</column>
      <column name="options">1</column>
      <column name="searchRegex">/[(hl|highlight|bg|background)=("|)(.*?)("|)](.*?)[/(hl|highlight|bg|background)]/is</column>
      <column name="replacement">&lt;span style="background-color: $3;"&gt;$5&lt;/span&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">9</column>
      <column name="bbcodeName">Image</column>
      <column name="options">1</column>
      <column name="searchRegex">/[img](.*?)[/img]/is</column>
      <column name="replacement">&lt;a href="$1" target="_BLANK"&gt;&lt;img src="$1" alt="image" class="embedImage" /&gt;&lt;/a&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">10</column>
      <column name="bbcodeName">Image (with Alternate Text)</column>
      <column name="options">1</column>
      <column name="searchRegex">/[img=("|)(.*?)("|)](.*?)[/img]/is</column>
      <column name="replacement">&lt;a href="$4" target="_BLANK"&gt;&lt;img src="$4" alt="$2" class="embedImage" /&gt;&lt;/a&gt;</column>
    </table>
    <table name="bbcode">
      <column name="bbcodeId">11</column>
      <column name="bbcodeName">Youtube Embed</column>
      <column name="options">1</column>
      <column name="searchRegex">/[youtube](.*?)[/youtube]/is</column>
      <column name="replacement">&lt;object width="420" height="255" wmode="transparent"&gt;&lt;param name="movie" value="http://www.youtube.com/v/$1=en&amp;amp;fs=1&amp;amp;rel=0&amp;amp;border=0"&gt;&lt;/param&gt;&lt;param name="allowFullScreen" value="true"&gt;&lt;/param&gt;&lt;embed src="http://www.youtube.com/v/$1&amp;amp;hl=en&amp;amp;fs=1&amp;amp;rel=0&amp;amp;border=0" type="application/x-shockwave-flash" allowfullscreen="true" width="420" height="255" wmode="opaque"&gt;&lt;/embed&gt;&lt;/object&gt;</column>
    </table>


