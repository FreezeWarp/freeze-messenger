{
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/iignatov/LightOpenID"
    }
  ],
  "require": {
    "google/apiclient" : "^2.0",
    "abraham/twitteroauth" : "0.7.4",
    "facebook/graph-sdk" : "5.6.1",
    "stevenmaguire/oauth2-microsoft" : "2.2.0",
    "rtheunissen/oauth2-reddit" : "*",
    "iignatov/lightopenid": "*",
    "google/recaptcha": "~1.1",
    "kwn/php-rdkafka-stubs": "^1.0.2",
    "bshaffer/oauth2-server-php" : "^1.10"
  },
  "autoload" : {
    "psr-4": {
      "Login\\" : "functions/Login/",
      "Cache\\" : "functions/Cache/",
      "Stream\\" : "functions/Stream/",
      "Database\\" : "functions/Database/",
      "Http\\" : "functions/Http/",
      "Fim\\" : "functions/Fim/"
    }
  }
}